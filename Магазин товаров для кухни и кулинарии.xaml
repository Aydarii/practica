<Window x:Class="pract.Магазин_товаров_для_кухни_и_кулинарии"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pract"
        mc:Ignorable="d"
        Title="Магазин_товаров_для_кухни_и_кулинарии" Height="450" Width="800" Background="LightBlue">
    <StackPanel>
        <StackPanel>
            <TextBlock Text="Личный кабинет" FontSize="25" FontWeight="ExtraBold" HorizontalAlignment="Center"></TextBlock>
        </StackPanel>
        <Image Height="211" Source="/logo-top-yameb (1)-Photoroom.png-Photoroom.png" Stretch="Fill" Width="488"/>
        <StackPanel>
            <Button Content="Каталог товаров" FontSize="20" FontWeight="Bold"  HorizontalAlignment="Center" Width="260" BorderThickness="4" BorderBrush="Black" Click="Button_Click"></Button>
        </StackPanel>
        <StackPanel>
            <Button Content="Корзина" FontSize="20" FontWeight="Bold" Width="287" BorderThickness="4" BorderBrush="Black"  Click="korzina_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
